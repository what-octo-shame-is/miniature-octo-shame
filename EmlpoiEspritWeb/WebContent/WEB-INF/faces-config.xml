<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<application>
		<message-bundle>resources.application</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
	</application>
<managed-bean>
  <managed-bean-name>controllerGroupe</managed-bean-name>
  <managed-bean-class>TestControllers.ControllerGroupe</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllerAffectation</managed-bean-name>
  <managed-bean-class>tn.esprit.emploi.affectation.ControllerAffectation</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>resoudre</managed-bean-name>
  <managed-bean-class>resolution.Resoudre</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllerResolut</managed-bean-name>
  <managed-bean-class>resolution.ControllerResolution</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllerDisponibilite</managed-bean-name>
  <managed-bean-class>TestControllers.ControllerDisponibilite</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllerEnseignant</managed-bean-name>
  <managed-bean-class>TestControllers.ControllerEnseignant</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllerCreneau</managed-bean-name>
  <managed-bean-class>TestControllers.ControllerCreneau</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>moduleContr</managed-bean-name>
  <managed-bean-class>TestControllers.ModuleContr</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllers</managed-bean-name>
  <managed-bean-class>TestControllers.Controllers</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllerAdmin</managed-bean-name>
  <managed-bean-class>TestControllers.ControllerAdmin</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>controllersGr</managed-bean-name>
  <managed-bean-class>TestControllers.ControllersGr</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>moduleContrEns</managed-bean-name>
  <managed-bean-class>TestControllers.ModuleContrEns</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>    
  <managed-bean>
  <managed-bean-name>treeBean</managed-bean-name>
  <managed-bean-class>controlleurModification.TreeBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
<navigation-rule>
  <from-view-id>/AuthentificationAdmin.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>yess</from-outcome>
   <to-view-id>/exemple.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>noo</from-outcome>
   <to-view-id>/erreur.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/AuthentificationEnseignant.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>noo</from-outcome>
   <to-view-id>/erreur.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>yesEns</from-outcome>
   <to-view-id>/EspaceEnseignant.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
